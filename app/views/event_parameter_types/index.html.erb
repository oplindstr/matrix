<p id="notice"><%= notice %></p>

<h1>Listing Event Parameter Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_parameter_types.each do |event_parameter_type| %>
      <tr>
        <td><%= event_parameter_type.name %></td>
        <td><%= link_to 'Show', event_parameter_type %></td>
        <td><%= link_to 'Edit', edit_event_parameter_type_path(event_parameter_type) %></td>
        <td><%= link_to 'Destroy', event_parameter_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event parameter type', new_event_parameter_type_path %>
