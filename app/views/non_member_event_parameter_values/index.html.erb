<p id="notice"><%= notice %></p>

<h1>Listing Non Member Event Parameter Values</h1>

<table>
  <thead>
    <tr>
      <th>Event parameter</th>
      <th>Non member signup</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @non_member_event_parameter_values.each do |non_member_event_parameter_value| %>
      <tr>
        <td><%= non_member_event_parameter_value.event_parameter_id %></td>
        <td><%= non_member_event_parameter_value.non_member_signup_id %></td>
        <td><%= non_member_event_parameter_value.value %></td>
        <td><%= link_to 'Show', non_member_event_parameter_value %></td>
        <td><%= link_to 'Edit', edit_non_member_event_parameter_value_path(non_member_event_parameter_value) %></td>
        <td><%= link_to 'Destroy', non_member_event_parameter_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Non member event parameter value', new_non_member_event_parameter_value_path %>
